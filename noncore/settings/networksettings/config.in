config NETWORKSETUP
  boolean "Networksetup"
  default "y"
  depends ( LIBQPE || LIBQPE-X11 ) && LIBOPIE

config NETWORKSETUP-CORE
  boolean
  default "y" if NETWORKSETUP
  depends ( LIBQPE || LIBQPE-X11 ) && LIBOPIE && NETWORKSETUP && INTERFACES

  source noncore/net/networksetttings/wlan/config.in
  source noncore/net/networksetttings/interfaces/config.in
