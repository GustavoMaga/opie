	$(QMAKE) -o Makefile opie-reader.pro
qmake: 
	@$(QMAKE) -o Makefile opie-reader.pro

dist: 
	@mkdir -p .tmp/reader && $(COPY_FILE) --parents $(SOURCES) $(HEADERS) $(FORMS) $(DIST) .tmp/reader/ && $(COPY_FILE) --parents ../../../i18n/de/reader.ts ../../../i18n/nl/reader.ts ../../../i18n/da/reader.ts ../../../i18n/xx/reader.ts ../../../i18n/en/reader.ts ../../../i18n/es/reader.ts ../../../i18n/fr/reader.ts ../../../i18n/hu/reader.ts ../../../i18n/ja/reader.ts ../../../i18n/ko/reader.ts ../../../i18n/no/reader.ts ../../../i18n/pl/reader.ts ../../../i18n/pt/reader.ts ../../../i18n/pt_BR/reader.ts ../../../i18n/sl/reader.ts ../../../i18n/zh_CN/reader.ts ../../../i18n/zh_TW/reader.ts .tmp/reader/ && ( cd `dirname .tmp/reader` && $(TAR) reader.tar reader && $(GZIP) reader.tar ) && $(MOVE) `dirname .tmp/reader`/reader.tar.gz . && $(DEL_DIR) .tmp/reader

