Keycode = F9
UserText = Contacts Button
Icon = devicebuttons/jornada56x_contact
PressedActionChannel = addressbook
PressedActionMessage = raise()
HeldActionChannel = addressbook
HeldActionMessage = beamBusinessCard()

[Button2]
Keycode = F11
UserText = Todo Button
