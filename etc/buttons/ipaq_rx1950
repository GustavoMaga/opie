UserText = Calendar Button
Icon = devicebuttons/ipaq_calendar
PressedActionChannel = datebook
PressedActionMessage = nextView()
HeldActionChannel = today
HeldActionMessage = raise()

[Button1]
Keycode = F2
UserText = Contacts Button
Icon = devicebuttons/ipaq_contact
PressedActionChannel = addressbook
PressedActionMessage = raise()
HeldActionChannel = addressbook
HeldActionMessage = beamBusinessCard()

