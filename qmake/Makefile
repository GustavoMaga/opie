CC		= gcc
CXX		= g++

#qmake code
OBJS=project.o main.o makefile.o unixmake2.o unixmake.o borland_bmake.o \
     msvc_nmake.o msvc_dsp.o msvc_vcproj.o option.o winmakefile.o \
